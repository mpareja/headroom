{
  "name": "headroom",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": true,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "scripts": {
    "clean": "rimraf .nyc_output coverage",
    "cov": "opener coverage/lcov-report/index.html",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run clean && npm run lint && npm run security && nyc npm run test-only",
    "test-only": "mocha --recursive --reporter ${MOCHA_REPORTER:-spec}",
    "watch-test": "mocha --recursive -w",
    "security": "nsp check --output summary"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "nsp": "^3.2.1",
    "nyc": "^11.4.1",
    "opener": "^1.4.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "standard": "^11.0.0"
  }
}
