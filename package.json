{
  "name": "headroom",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": true,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "standard": {
    "env": [
      "mocha",
      "node"
    ]
  },
  "scripts": {
    "clean": "rimraf .nyc_output coverage",
    "cov": "opener coverage/lcov-report/index.html",
    "lint": "standard --env mocha",
    "test": "npm run clean && npm run lint && nyc npm run test-only",
    "test-only": "mocha --recursive --reporter ${MOCHA_REPORTER:-spec}",
    "watch-test": "mocha --recursive -w"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "chai": "^4.2.0",
    "memfs": "^3.2.0",
    "mocha": "^7.2.0",
    "nock": "^12.0.3",
    "nyc": "^15.1.0",
    "opener": "^1.5.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "isomorphic-unfetch": "^3.0.0",
    "p-queue": "^6.4.0",
    "standard": "^14.3.4"
  }
}
